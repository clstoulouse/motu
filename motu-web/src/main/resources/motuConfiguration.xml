		<queues id="queue2" description="bulk queue" batch="false">
			<maxThreads>2</maxThreads>
			<maxPoolSize>-1</maxPoolSize>
			<dataThreshold>1024</dataThreshold>
			<lowPriorityWaiting>2</lowPriorityWaiting>
		</queues>
