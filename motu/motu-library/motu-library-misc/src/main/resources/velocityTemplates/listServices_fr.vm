<html>
<head>
  <title>Services d'accès publiés</title>
</head>

<style>
	h1
	{
	font: 18pt "Arial", "Helvetica", sans-serif;
	font-weight: bold;
	color:#000099;
    padding-top: 20px;
	}
	h2
	{
	font: 14pt "Arial", "Helvetica", sans-serif;
	font-weight: bold;
	color:#000099;
    padding-top: 10px;
	}
</style> 

<body>
	<h1>Services d'accès publiés/h1>
	<table>
	#foreach( $service in $serviceList )
	<tr>
		<td>	
	    <h2>
		<a href=?action=listcatalog&service=${service.getNameEncoded()}>
	 		 ${service.getGroup()}&nbsp;${service.getDescription()} 
	    </h2>
		</td>
	</tr>
	#end
	</table>	   
</body>
</html>
