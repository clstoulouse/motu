## ------------------------------ aviso_uk_printOtherServices --------------------------------
#macro(aviso_uk_printOtherServices)
	#set($listServicesSize = ${service.getSameGroupServices().size()})
	#set($i = 0)
	#foreach( $s in ${service.getSameGroupServices()} )	
		#set($i = $i+1)
		#if(${service.getName().equalsIgnoreCase(${s.getName()})})
			${s.getDescription()}
		#elseif($body_template.equalsIgnoreCase($templateCatalog))			
			<a href="?action=listcatalog&service=${s.getNameEncoded()}">${s.getDescription()}</a>
		#end
		#if (($i != $listServicesSize) && ($body_template.equalsIgnoreCase($templateCatalog)))
			|
		#end
	#end
#end
## ------------------------------ aviso_uk_printSubTitleCatalog --------------------------------
#macro(aviso_uk_printSubTitleCatalog)
	Inventory
#end

## ------------------------------ aviso_uk_printSubTitleProductMetadata --------------------------------
#macro(aviso_uk_printSubTitleProductMetadata)
	<a href="?action=listcatalog&service=${service.getNameEncoded()}">Inventory</a>
	$imgParent
	Metadata

#end

## ------------------------------ aviso_uk_printSubTitleProductDownloadHome --------------------------------
#macro(aviso_uk_printSubTitleProductDownloadHome)
	<a href="?action=listcatalog&service=${service.getNameEncoded()}">Inventory</a>
	$imgParent
	<a href="?action=listproductmetadata&service=${service.getNameEncoded()}&product=${product.getProductMetaData().getProductId()}">Metadata</a>
	$imgParent
	Download
#end

