<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!DOCTYPE rdf:RDF [
<!ENTITY atoll "http://atoll.cls.fr/2009/resource/individual/atoll#" >
]>
<wps:Execute service="WPS" version="1.0.0"
	xmlns:wps="http://www.opengis.net/wps/1.0.0" xmlns:ows="http://www.opengis.net/ows/1.1"
	xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.opengis.net/wps/1.0.0
http://schemas.opengis.net/wps/1.0.0/wpsExecute_request.xsd">

	<ows:Identifier>ExtractData</ows:Identifier>
	<wps:DataInputs>
		<wps:Input>
			<ows:Identifier>service</ows:Identifier>
			<ows:Title>Name of the Motu service</ows:Title>
			<ows:Abstract>This parameter specifies the name of the Motu service as described in the Motu configuration</ows:Abstract>
			<wps:Data>
				<wps:LiteralData dataType="string">&atoll;motu-opendap-mercator</wps:LiteralData>
			</wps:Data>
		</wps:Input>
		<wps:Input>
			<ows:Identifier>product</ows:Identifier>
			<ows:Title>The product identifier (name)</ows:Title>
			<wps:Data>
				<wps:LiteralData dataType="string">mercatorPsy3v2_nat_mean_best_estimate</wps:LiteralData>
			</wps:Data>
		</wps:Input>
		<wps:Input>
			<ows:Identifier>starttime</ows:Identifier>
			<ows:Title>The start coverage datetime</ows:Title>
			<wps:Data>
				<wps:LiteralData dataType="datetime">2009-04-27T10:00:00.000+02:00</wps:LiteralData>
			</wps:Data>
		</wps:Input>
		<wps:Input>
			<ows:Identifier>endtime</ows:Identifier>
			<ows:Title>The end coverage datetime</ows:Title>
			<wps:Data>
				<wps:LiteralData dataType="datetime">2009-04-28T00:00:00.000+02:00</wps:LiteralData>
			</wps:Data>
		</wps:Input>
		<wps:Input>
			<ows:Identifier>variable</ows:Identifier>
			<ows:Title>Name or identifier of the data</ows:Title>
			<wps:Data>
				<wps:LiteralData dataType="string">u</wps:LiteralData>
			</wps:Data>
		</wps:Input>
		<wps:Input>
			<ows:Identifier>geobbox</ows:Identifier>
			<ows:Title>The Latitude/Longitude bounding box</ows:Title>
			<wps:Data>
				<wps:BoundingBoxData>
					<ows:LowerCorner>10.0 -60.0</ows:LowerCorner>
					<ows:UpperCorner>45.0 120.0</ows:UpperCorner>
				</wps:BoundingBoxData>
			</wps:Data>
		</wps:Input>
		<wps:Input>
			<ows:Identifier>lowdepth</ows:Identifier>
			<ows:Title>The lower depth (z)</ows:Title>
			<wps:Data>
				<wps:LiteralData dataType="string">surface</wps:LiteralData>
			</wps:Data>
		</wps:Input>
		<wps:Input>
			<ows:Identifier>highdepth</ows:Identifier>
			<ows:Title>The higher depth (z)</ows:Title>
			<wps:Data>
				<wps:LiteralData dataType="string">150</wps:LiteralData>
			</wps:Data>
		</wps:Input>
	</wps:DataInputs>
</wps:Execute>

