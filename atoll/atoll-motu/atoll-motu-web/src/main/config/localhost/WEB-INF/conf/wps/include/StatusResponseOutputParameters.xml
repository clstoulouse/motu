<?xml version="1.0" encoding="UTF-8"?>
	<LiteralOutput>
		<Identifier>status</Identifier>
		<Title>status of a request</Title>
		<Abstract><![CDATA[
		1 = DONE, 
		2 = ERROR, 
		3 = PENDING, 
		4 = IN PROGRESS
		]]>
		</Abstract>
		<DataType reference="http://www.w3.org/TR/xmlschema-2/#integer">statusModeType</DataType>
	</LiteralOutput>
